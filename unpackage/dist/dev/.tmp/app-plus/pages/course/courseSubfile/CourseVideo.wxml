<view id="CourseVideo" class="data-v-f9c1e2ec" bind:__l="__l"><view class="AppAllExplain data-v-f9c1e2ec"><text class="data-v-f9c1e2ec">{{msg}}</text></view><view class="videoBox data-v-f9c1e2ec"><video id="myVideo" src="{{videoUrl}}" autoplay="true" controls class="data-v-f9c1e2ec"></video></view><block wx:if="{{chapterTitle!==''}}"><view class="knowledgePoint data-v-f9c1e2ec"><view class="titleName data-v-f9c1e2ec">知识要点：</view><view class="chapterTitle data-v-f9c1e2ec"><text class="titleText data-v-f9c1e2ec">{{chapterTitle}}</text></view><block wx:for="{{content}}" wx:for-item="knowledgePointItem" wx:for-index="knowledgePointIndex" wx:key="knowledgePointIndex"><view data-event-opts="{{[['tap',[['playVideo',['$0'],[[['content','',knowledgePointIndex,'ThirdContentUrl']]]]]]]}}" class="singleKnowledgePoint data-v-f9c1e2ec" bindtap="__e">{{knowledgePointItem.ContentName}}</view></block></view></block><view class="catalogueBox data-v-f9c1e2ec"><view class="navList data-v-f9c1e2ec"><view class="{{['navBox data-v-f9c1e2ec',(showCard===2)?'navActive':'']}}"><text class="data-v-f9c1e2ec">课程目录</text></view><navigator url="{{'CourseAdvertise?CourseID='+courseID}}" hover-class="navigator-hover" open-type="navigate" class="data-v-f9c1e2ec"><view class="{{['navBox data-v-f9c1e2ec',(showCard===1)?'navActive':'']}}"><text class="data-v-f9c1e2ec">课程概述</text></view></navigator></view><uni-collapse class="data-v-f9c1e2ec" vue-slots="{{['default']}}"><block wx:for="{{catalogueList}}" wx:for-item="courseSectionItem" wx:for-index="courseSectionIndex" wx:key="courseSectionIndex"><uni-collapse-item title="{{courseSectionIndex+1+'.'+courseSectionItem.ChapterTitle}}" class="data-v-f9c1e2ec" vue-slots="{{['default']}}"><block wx:for="{{courseSectionItem.son}}" wx:for-item="sonCatalogueItem" wx:for-index="sonCatalogueIndex" wx:key="sonCatalogueIndex"><block wx:if="{{courseSectionItem.son[courseSectionIndex].son===undefined}}"><view data-event-opts="{{[['tap',[['courseKnowledgePoint',['$0'],[